<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="topleft">
		<line>
            <str>Day {day}, {mctime} (</str>
            <if>
                <var>daytime</var>
                <str>$eDay</str>
                <str>$8Night</str>
            </if>
            <str>$f)</str>
        </line>
        <line>
            <str>Biome: $e{biome}$f</str>
            <if>
                <var>slimes</var>
                <str> ($aSlime$f)</str>
                <str> </str>
            </if>
        </line>
        <line>
            <str>Light: </str>
            <max>
                <var>light</var>
                <num>7.5</num>
                <str>$e</str>
                <str>$c</str>
            </max>
            <var>light</var>
            <str>$r (eye: </str>
            <max>
                <var>lighteye</var>
                <num>7.5</num>
                <str>$e</str>
                <str>$c</str>
            </max>
            <var>lighteye</var>
            <str>$r)</str>
        </line>
        <line>
            <str>$fEXP: $e{xpthislevel}$f / $e{xpcap}$f ($e</str>
            <round>
                <pct>
                    <var>xpthislevel</var>
                    <var>xpcap</var>
                </pct>
                <num>0</num>
            </round>
            <str>%$f)</str>
        </line>
        <line>
            <str>FPS: </str>
            <operation>
                <str>gt</str>
                <var>fps</var>
                <num>60</num>
                <num>45</num>
                <num>30</num>
                <num>15</num>
                <num>0</num>
                <str>$a</str>
                <str>$e</str>
                <str>$6</str>
                <str>$c</str>
                <str>$4</str>
                <str/>
            </operation>
            <var>fps</var>
        </line>
        <line>
            <str>Entities: $e{entitiesrendered}$f / $e{entitiestotal}$f</str>
        </line>
    </lines>
    <lines at="bottomleft">
        <line>
            <var>mainhandicon</var>
            <str> </str>
            <operation>
                <str>ge</str>
                <pct>
                    <var>mainhanddamageleft</var>
                    <var>mainhandmaxdamage</var>
                </pct>
                <num>50</num>
                <num>20</num>
                <num>10</num>
                <num>0</num>
                <str>$e</str>
                <str>$6</str>
                <str>$c</str>
                <str>$4</str>
                <str/>
            </operation>
            <max>
                <var>mainhanddamageleft</var>
                <num>0.5</num>
                <str>{mainhanddamageleft}$f / $e{mainhandmaxdamage}</str>
                <var>mainhandname</var>
            </max>
        </line>
        <line>
            <var>offhandicon</var>
            <str> </str>
            <operation>
                <str>ge</str>
                <pct>
                    <var>offhanddamageleft</var>
                    <var>offhandmaxdamage</var>
                </pct>
                <num>50</num>
                <num>20</num>
                <num>10</num>
                <num>0</num>
                <str>$e</str>
                <str>$6</str>
                <str>$c</str>
                <str>$4</str>
                <str/>
            </operation>
            <max>
                <var>offhanddamageleft</var>
                <num>0.5</num>
                <str>{offhanddamageleft}$f / $e{offhandmaxdamage}</str>
                <var>offhandname</var>
            </max>
        </line>
        <line>
            <str> </str>
        </line>
    </lines>
</config>